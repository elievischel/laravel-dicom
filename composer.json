{
    "name": "php-dicom/getallheaders",
    "description": "PHP DICOM for LARAVEL",
    "type": "composer-plugin",
    "license": "MIT",
    "keywords": ["http", "discovery", "client", "adapter", "message", "factory", "psr7", "psr17"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "elie vischel",
            "email": "elie.vischel@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "composer-plugin-api": "^1.0|^2.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Http\\Discovery\\": "src/"
        },
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Http\\Discovery\\": "spec/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpspec run",
            "vendor/bin/simple-phpunit --group NothingInstalled"
        ],
        "test-ci": "vendor/bin/phpspec run -c phpspec.ci.yml"
    },
    "extra": {
        "class": "Http\\Discovery\\Composer\\Plugin",
        "plugin-optional": true
    },
    "prefer-stable": true,
    "minimum-stability": "beta"
}
